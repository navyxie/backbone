.selectItemTable
	width 600px
	position fixed
	left 50%
	margin-left -300px
	top 100px
	background-color #fff
	z-index 99
	display none
	td
		cursor pointer
	.op
		width 40px
	.size
		width 80px
	.check
		height 100%
		display block
		background url(../images/notchecked.png) no-repeat center
		&.selected{
			background-image url(../images/checked.png)
		}